{
  "name": "base-react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_BUILD_TYPE=LOCAL vite --port 3000 --host",
    "build": "tsc -b && vite build",
    "build:dev": "tsc && VITE_BUILD_TYPE=DEV vite build",
    "build:stg": "tsc && VITE_BUILD_TYPE=STG vite build",
    "build:prod": "tsc && VITE_BUILD_TYPE=PROD vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npx serve -s dist"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.53.2",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@locked-dobby/eslint-config": "^1.1.5-canary.26",
    "@locked-dobby/prettier-config": "^1.1.5-canary.26",
    "@mui/icons-material": "^6.0.1",
    "@mui/material": "^6.0.1",
    "@tanstack/eslint-plugin-query": "^5.53.0",
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-html": "^3.2.2"
  }
}
